# Supabase Configuration
# Obter em: https://supabase.com/dashboard/project/_/settings/api
SUPABASE_URL=https://YOUR_PROJECT.supabase.co
SUPABASE_KEY=sb_publishable_YOUR_PUBLISHABLE_KEY
SUPABASE_SERVICE_ROLE_KEY=sb_secret_YOUR_SECRET_KEY

# Database URLs (Opcional - para migrations)
# Obter em: https://supabase.com/dashboard/project/_/settings/database
DATABASE_URL="postgresql://postgres.PROJECT:PASSWORD@HOST:6543/postgres?pgbouncer=true"
DIRECT_URL="postgresql://postgres.PROJECT:PASSWORD@HOST:5432/postgres"

# Frontend (Vite) - Mesmas credenciais p√∫blicas
VITE_SUPABASE_URL=https://YOUR_PROJECT.supabase.co
VITE_SUPABASE_KEY=sb_publishable_YOUR_PUBLISHABLE_KEY

# Server Configuration
PORT=5000
NODE_ENV=production
